{
  "singleQuote": true,
  "jsxSingleQuote": false,
  "tabWidth": 2,
  "semi": true,
  "printWidth": 120,
  "arrowParens": "avoid",
  "jsxBracketSameLine": false,
  "importOrder": ["^react(.*)", "<THIRD_PARTY_MODULES>", "^./components/(.*)$", "^./*.*.scss", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "overrides": [
    {
      "files": "**/*.scss",
      "options": {
        "singleQuote": false
      }
    }
  ],
  "bracketSpacing": true,
  "proseWrap": "never",
  "quoteProps": "consistent"
}
